<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote  name="github"
           fetch="git://github.com" />

  <default revision="refs/heads/master"
           remote="github"
           sync-c="true"
           sync-j="4" />

  <project path="repo" name="ccosby/jenkins-ruby-plugins">
    <copyfile src=".rvmrc" dest=".rvmrc"/>
  </project>
  <project path="buddycloud-for-jenkins" name="lloydwatkin/buddycloud-for-jenkins" />
  <project path="gitlab-hook-plugin" name="jenkinsci/gitlab-hook-plugin" />
  <project path="jenkins-git-notes-plugin" name="swipely/jenkins-git-notes-plugin" />
  <project path="pathignore-plugin" name="jenkinsci/pathignore-plugin" />
  <project path="rvm-plugin" name="jenkinsci/rvm-plugin" />
  <project path="vagrant-plugin" name="rtyler/vagrant-plugin" />
  <project path="yammer-plugin" name="jenkinsci/yammer-plugin" />

</manifest>

